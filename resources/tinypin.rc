// Menu
IDM_TRAY MENU 
CHARACTERISTICS 0x00000001
BEGIN
    POPUP "(tray)"
    BEGIN
        MENUITEM "&Enter pin mode",             CM_NEWPIN
        MENUITEM "&Remove all pins",            CM_REMOVEPINS
        MENUITEM SEPARATOR
        MENUITEM "&Options",                 CM_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&About",                      CM_ABOUT
        MENUITEM SEPARATOR
        MENUITEM "&Close TinyPin",             CM_CLOSE
    END
END

